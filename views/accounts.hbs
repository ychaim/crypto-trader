<h1 xmlns="http://www.w3.org/1999/html">{{title}}</h1>

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <table class="table table-striped">

                <thead>
                <tr>
                    <th>Currency</th>
                    <th>Balance</th>
                    <th>Available</th>
                    <th>Hold</th>
                    <th>Profile Id</th>
                </tr>
                </thead>
                <tbody>

                {{#each data}}
                    <tr>
                        <td>{{this.currency}}</td>
                        <td>{{this.balance}}</td>
                        <td>{{this.available}}</td>
                        <td>{{this.hold}}</td>
                        <td>{{this.profile_id}}</td>
                    </tr>
                {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>